            <Font name="System Bold" size="14.0" />
        </font>
        <graphic>
            <ImageView fitHeight="37.0" fitWidth="59.0" layoutX="762.0" layoutY="668.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../../../icon/add.png" />
                </image>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="saveButton" layoutX="921.0" layoutY="668.0" mnemonicParsing="false" onAction="#saveButton" prefHeight="37.0" prefWidth="43.0">
