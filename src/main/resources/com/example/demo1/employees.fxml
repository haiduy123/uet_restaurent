<AnchorPane fx:id="employeeContainer" prefHeight="757.0" prefWidth="1158.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="employees.Controller">
<children>
    <TableView fx:id="employeeTableView" layoutX="14.0" layoutY="305.0" prefHeight="322.0" prefWidth="1056.0">
        <columns>
            <TableColumn fx:id="eId_Column" prefWidth="44.0" text="ID" />
            <TableColumn fx:id="eLastName_Column" prefWidth="131.0" styleClass="centered-table-column" text="LastName" />
            <TableColumn fx:id="eFirstName_Column" prefWidth="87.0" styleClass="centered-table-column" text="FirstName" />
            <TableColumn fx:id="eBirthday_Column" prefWidth="113.0" styleClass="centered-table-column" text="Birthday" />
